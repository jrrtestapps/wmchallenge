# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'WeedmapsChallenge' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WeedmapsChallenge
  pod 'Alamofire', '4.7.3'
  # Pods to better support MVVM and functional programming
  pod 'RxSwift', '4.4.2'
  pod 'RxCocoa', '4.4.2'

  target 'WeedmapsChallengeTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '4.4.2'
    pod 'RxTest', '4.4.2'
  end

  target 'WeedmapsChallengeUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
